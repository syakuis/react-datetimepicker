{
  "name": "react-datetimepicker-syaku",
  "version": "1.0.8",
  "description": "react 용 날짜와 시간 선택기. flatpickr 을 이용하여 개발하였다",
  "author": "Syaku (최석균) <http://syaku.tistory.com>",
  "url": "http://syakuis.github.io/demo/react-datetimepicker",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/syakuis/react-datetimepicker"
  },
  "keywords": [
    "syaku",
    "react",
    "datetimepicker",
    "calendar",
    "date",
    "time",
    "picker",
    "lightweight",
    "flatpickr"
  ],
  "main": "dist/react-datetimepicker.js",
  "jest": {
    "verbose": true,
    "testMatch": [
      "**/__tests__/**/*.js?(x)"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest"
    },
    "globals": {
      "NODE_ENV": "test"
    }
  },
  "config": {
    "filename": "react-datetimepicker",
    "library": "ReactDatetimePicker",
    "publicPath": "",
    "output": "dist",
    "src": "src",
    "entry": "./src/index.js",
    "port": 8089,
    "externals": {
      "react": {
        "root": "React",
        "commonjs2": "react",
        "commonjs": "react",
        "amd": "react"
      },
      "react-dom": {
        "root": "ReactDOM",
        "commonjs2": "react-dom",
        "commonjs": "react-dom",
        "amd": "react-dom"
      },
      "flatpickr": {
        "root": "flatpickr",
        "commonjs2": "flatpickr",
        "commonjs": "flatpickr",
        "amd": "flatpickr"
      }
    }
  },
  "scripts": {
    "jest": "jest",
    "build": "NODE_ENV='production' webpack --profile --colors --progress",
    "demo": "NODE_ENV='production' webpack --config webpack.config.demo.js --profile --colors --progress",
    "dev": "NODE_ENV='development' webpack-dev-server --config webpack.config.dev.js --colors --progress"
  },
  "devDependencies": {
    "babel-core": "^6.23.1",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^21.0.2",
    "babel-loader": "^7.0.0",
    "babel-plugin-dynamic-import-webpack": "^1.0.1",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-3": "^6.22.0",
    "bootstrap": "^3.3.7",
    "bundle-loader": "^0.5.5",
    "clean-webpack-plugin": "^0.1.16",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.4",
    "eslint": "^4.6.1",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-import-resolver-webpack": "^0.8.1",
    "eslint-loader": "^1.6.3",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jest": "^21.1.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^7.0.1",
    "extract-text-webpack-plugin": "^2.0.0",
    "file-loader": "^0.11.2",
    "flatpickr": "^4.0.4",
    "font-awesome": "^4.7.0",
    "html-webpack-plugin": "^2.28.0",
    "jest": "^21.1.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "style-loader": "^0.18.2",
    "stylelint": "^7.12.0",
    "stylelint-config-standard": "^16.0.0",
    "stylelint-webpack-plugin": "^0.7.0",
    "uglifyjs-webpack-plugin": "^1.1.8",
    "url-loader": "^0.5.9",
    "webpack": "2.6.1",
    "webpack-bundle-analyzer": "^2.3.0",
    "webpack-dev-server": "^2.4.2",
    "webpack-merge": "^4.1.0"
  },
  "dependencies": {
    "prop-types": "^15.5.10"
  },
  "peerDependencies": {
    "flatpickr": "^4.0.0 || ^3.5.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0"
  }
}
